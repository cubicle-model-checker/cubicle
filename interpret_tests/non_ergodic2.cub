type t = A | B | C | D | E


array P[proc] : t
var X : bool


init(i) { X = True && P[i] = A }


transition start1(i)
requires { X = True }
{ P[i] :=  B }

transition start2(i)
requires { X = True }
{  P[i] :=  D }


transition next1(i)
requires { P[i] = B }
{ P[i] := C; X := False }


transition loop1(i)
requires { P[i] = C }
{ P[i] := B }


transition next2(i)
requires {  P[i] = D }
{ P[i] := E; X := False }


transition loop2(i)
requires {  P[i] = E }
{ P[i] := D } 