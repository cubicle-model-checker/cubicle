  UniMsg_Cmd[src] := UNI_Get;
  UniMsg_Proc[src] := Dir_HeadPtr;
  (* undefine UniMsg_Data[src]; *)
  FwdCmd := UNI_Get;
  PendReqSrc := src;
  PendReqCmd := UNI_Get;
  Collecting := False;
