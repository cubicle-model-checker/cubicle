var P1 : bool
var P2 : bool

init {
    P1 = False && P2 = False
}

transition t1 ()
requires { P1 = False }
{ P1 := True; }


transition t2()
requires { P2 = False }
{ P2 := True; }   

transition t3 ()
requires { P1 = True && P2 = False }
{ P1 := False; }


transition t4 ()
requires { P2 = True && P1 = False }
{ P2 := False; }
