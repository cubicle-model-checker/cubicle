init(i) { PC[i] = A1 && X = 0 && Y = 0 && Z = 0 && T5 = 0 && T6 = 0 }

transition t1(i)
requires {PC[i] = A1} {PC[i] := A2}

transition t2(i)
requires {PC[i] = A2} {PC[i] := A3}

transition t4(i j)
requires {PC[i] = A3 && PC[j] <> A3} { Y := Y + 1; PC[i] := A1 }

transition t(i)
requires {PC[i] = A3 && forall_other j. PC[j] = A3} { X := X+1; PC[k] := case | _ : A4} (*rare state*)

transition t5(i)
requires { PC[i] = A4 }
{ PC[i] := A5; T5 := T5 + 1 }

transition t6(i)
requires { PC[i] = A4}
{ PC[i] := A1; T6 := T6 + 1 }

transition t7(i)
requires { PC[i] = A5 && forall_other j. PC[j] = A5} { Z := Z + 1;PC[j] := case | _ : A1 }



