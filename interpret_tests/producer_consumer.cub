var Mutex : lock

var Full : semaphore

var Empty : semaphore

init() { Mutex = 1 && Full = 0 && Empty = 5 }


(*--Produce--*)

transition produce_one(i)
requires{ PC[i] = PutWant } 
{ acquire(Empty,i);
  PC[i] := Put}

