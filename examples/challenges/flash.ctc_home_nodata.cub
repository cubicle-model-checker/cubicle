{
  UniMsg_Cmd[src] := UNI_Nak;
  UniMsg_Proc[src] := Home;
  (* undefine UniMsg_Data[src]; *)
}


(*--------------------- Muphi rule NI_Local_Get_Get ---------------------*)

transition ni_Local_Get_Get_Head (src)
requires { Home <> src &&
  	   UniMsg_Cmd[src] = UNI_Get &&
  	   UniMsg_Proc[src] = Home &&
  	   RpMsg_Cmd[src] <> RP_Replace &&
	   Dir_Pending = False &&
